{
  "name": "@actionbookdev/db",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "migrate": "drizzle-kit migrate",
    "migrate:generate": "drizzle-kit generate",
    "migrate:push": "tsx scripts/safe-push.ts",
    "migrate:push:force": "drizzle-kit push",
    "migrate:check": "drizzle-kit check",
    "studio": "NODE_TLS_REJECT_UNAUTHORIZED=0 drizzle-kit studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.44.7",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "drizzle-kit": "^0.31.8",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
