{
  "manifest_version": 3,
  "name": "Actionbook",
  "version": "0.1.1",
  "description": "Bridge between Actionbook CLI and your browser for AI-powered automation",
  "key": "",
  "permissions": [
    "debugger",
    "tabs",
    "activeTab",
    "scripting",
    "offscreen",
    "storage",
    "nativeMessaging",
    "cookies"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
