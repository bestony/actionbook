{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Actionbook",
  "colors": {
    "primary": "#475569",
    "light": "#cbd5e1",
    "dark": "#1e293b"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["index", "quickstart", "openclaw"]
          },
          {
            "group": "CLI",
            "pages": [
              "guides/installation",
              "guides/skills",
              "api-reference/cli",
              "guides/browser"
            ]
          },
          {
            "group": "Advanced",
            "pages": [
              "examples",
              "guides/mcp-server",
              "guides/sdk-integration",
              "community/contributing"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/actionbook/actionbook",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://actionbook.dev/discord",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://actionbook.dev/dashboard"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity"]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/actionbook",
      "discord": "https://actionbook.dev/discord",
      "x": "https://x.com/ActionbookHQ"
    }
  }
}
