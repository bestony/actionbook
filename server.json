{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "io.github.actionbook/actionbook",
  "title": "Actionbook",
  "description": "Up-to-date action manuals and DOM cache for any website",
  "repository": {
    "url": "https://github.com/actionbook/actionbook",
    "source": "github"
  },
  "version": "1.0.0",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "@actionbookdev/mcp",
      "version": "0.0.1",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "description": "API key for authentication",
          "isRequired": false,
          "format": "string",
          "isSecret": true,
          "name": "ACTIONBOOK_API_KEY"
        }
      ]
    }
  ]
}
